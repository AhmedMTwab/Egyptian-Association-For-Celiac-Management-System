@model HealthInsurance
<div style="
	 background: rgba(132, 134, 127, .5);
	 box-sizing: border-box;
	 box-shadow: 0 15px 25px rgba(0, 0, 0, .6);
	 border-radius: 10px;">
	<table class="table table-border">
		<thead>
			<tr itemscope="raw">
				<th itemscope="col">Insurance Id</th>
				<th itemscope="col">Insurance Name</th>
				<th itemscope="col">License Code</th>
				<th itemscope="col">PhoneNumbers</th>
				<th itemscope="col">Address</th>
			</tr>
		</thead>
		<tbody>
			
				<tr itemscope="raw">
					<td>
						@Model.InsuranceId
					</td>

					<td>
					@Model.Name
					</td>
				<td>
					@Model.LicenseCode
				</td>
					<td>
						@foreach (var phone in @Model.PhoneNumbers)
						{
							<p>
								@phone.PhoneNumber
							</p>
						}
					</td>
					<td>
						@foreach (var address in @Model.addresses)
						{
							<p>
								@address.Address
							</p>
						}
					</td>
					<td>
					<a asp-action="Details" asp-controller="Insurance" asp-route-Id="@Model.InsuranceId">Details</a>
					</td>

					<td>
					<a asp-action="EditInsurance" asp-controller="Insurance" asp-route-Id="@Model.InsuranceId">Edit</a>
					</td>
					<td>
					<a asp-action="DeleteInsurance" asp-controller="Insurance" asp-route-Id="@Model.InsuranceId">Delete</a>
					</td>
				</tr>

			
		</tbody>
	</table>
</div>