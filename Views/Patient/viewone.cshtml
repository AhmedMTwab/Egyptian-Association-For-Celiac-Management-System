@model Patient
<div style="
	 background: rgba(132, 134, 127, .5);
	 box-sizing: border-box;
	 box-shadow: 0 15px 25px rgba(0, 0, 0, .6);
	 border-radius: 10px;">
	<table class="table table-border">
		<thead>
			<tr itemscope="raw">
				<th itemscope="col">Patient Id</th>
				<th itemscope="col">Patient Name</th>
				<th itemscope="col">Date Of Birth</th>
				<th itemscope="col">ssn</th>
				<th itemscope="col">Blood Type</th>
			</tr>
		</thead>
		<tbody>
				<tr itemscope="raw">
					<td>
						@Model.PatientId
					</td>

					<td>
						@Model.PatientName
					</td>

					<td>
						@Model.Dob
					</td>

					<td>
					@Model.Ssn
					</td>

					<td>
					@Model.PatientBloodtype
					</td>

					<td>
						<a href="">Edit</a>
					</td>
					<td>
						<a href="">Delete</a>
					</td>
				</tr>

			}
		</tbody>
	</table>
</div>