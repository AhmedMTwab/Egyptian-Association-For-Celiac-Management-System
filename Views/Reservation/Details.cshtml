@model Reservation
<div style="
	 background: rgba(132, 134, 127, .5);
	 box-sizing: border-box;
	 box-shadow: 0 15px 25px rgba(0, 0, 0, .6);
	 border-radius: 10px;">
	<table class="table table-border">
		<thead>
			<tr itemscope="raw">
				<th itemscope="col">Reservation Id</th>
				<th itemscope="col">Patient Id</th>
				<th itemscope="col">Clinic Id</th>
				<th itemscope="col">Reservation Date</th>
				<th itemscope="col">Reservation Time</th>
				<th itemscope="col">Book Date</th>
				<th itemscope="col">Book Time</th>
			</tr>
		</thead>
		<tbody>
			<tr itemscope="raw">
				<td>
					@Model.ReservationId
				</td>

				<td>
					@Model.PatientId
				</td>

				<td>
					@Model.ClinicId
				</td>

				<td>
					@Model.ReservationDate
				</td>
				<td>
					@Model.ReservationTime
				</td>
				<td>
					@Model.BookDate
				</td>
				<td>
					@Model.BookTime
				</td>
			</tr>

		</tbody>
	</table>
</div>